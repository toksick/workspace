 */
#include <stdio.h>
#include <strings.h>

int main(int argc, char **argv) {

	int array[5] = {3,4,6,7,9};
	int new;
	int j;
	int i;
	int var;
	printf("test\n");
	for (j = 0; j < 4; ++j) {
		printf("for j\n");
		int key = array[j];
		for (i = j+1; i <= 4; ++i) {
			if (array[i] <= key) {
				key = array[i];
				new = i;
			}
			printf("for i\n");
		}
		array[new] = array[j];
		array[j] = key;

	}
	for (var = 0; var < 4; ++var) {
		printf("%d\n",&array[var]);
	}
	return(0);
}



